## index.html
<%inherit file="base"/>

<%def name="render_input(input)">
        <input name="${input.name}" />
</%def>

<%def name="render_form(form)">
    <form id="${form.id}">
    % for input in form.inputs:
        ${render_input(input)}
    % endfor
    </form>        
</%def>

<%block name="header">
    this is some header content
</%block>

% for form in forms:
  ${render_form(form)}
% endfor